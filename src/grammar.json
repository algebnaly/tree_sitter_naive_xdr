{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "naive_xdr",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "statement"
      }
    },
    "statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "block_comment"
        },
        {
          "type": "SYMBOL",
          "name": "enum"
        },
        {
          "type": "SYMBOL",
          "name": "struct"
        },
        {
          "type": "SYMBOL",
          "name": "const_statement"
        },
        {
          "type": "SYMBOL",
          "name": "union"
        }
      ]
    },
    "block_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/*"
          },
          {
            "type": "PATTERN",
            "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
          },
          {
            "type": "STRING",
            "value": "/"
          }
        ]
      }
    },
    "enum": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_enum"
        },
        {
          "type": "SYMBOL",
          "name": "enum_name"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "SYMBOL",
          "name": "_enum_variant_dec"
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "keyword_enum": {
      "type": "STRING",
      "value": "enum"
    },
    "enum_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "_enum_variant_dec": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "REPEAT",
              "content": {
                "type": "SEQ",
                "members": [
                  {
                    "type": "STRING",
                    "value": ","
                  },
                  {
                    "type": "SYMBOL",
                    "name": "identifier"
                  },
                  {
                    "type": "STRING",
                    "value": "="
                  },
                  {
                    "type": "SYMBOL",
                    "name": "number"
                  }
                ]
              }
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "struct": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_struct"
        },
        {
          "type": "SYMBOL",
          "name": "struct_name"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "struct_member"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "keyword_struct": {
      "type": "STRING",
      "value": "struct"
    },
    "struct_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "struct_member": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type_specifier"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "type_specifier": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "const_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_const"
        },
        {
          "type": "SYMBOL",
          "name": "type_specifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "keyword_const": {
      "type": "STRING",
      "value": "const"
    },
    "union": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_union"
        },
        {
          "type": "SYMBOL",
          "name": "union_name"
        },
        {
          "type": "SYMBOL",
          "name": "keyword_switch"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "discriminant_declaration"
        },
        {
          "type": "STRING",
          "value": ")"
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "case_arm"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "default_arm"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "keyword_union": {
      "type": "STRING",
      "value": "union"
    },
    "keyword_switch": {
      "type": "STRING",
      "value": "switch"
    },
    "union_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "discriminant_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "type_specifier"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "case_arm": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_case"
        },
        {
          "type": "SYMBOL",
          "name": "discriminant_declaration"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "arm_declaration"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "default_arm": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword_default"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "arm_declaration"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "discriminant_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "arm_declaration": {
      "type": "SYMBOL",
      "name": "type_specifier"
    },
    "keyword_case": {
      "type": "STRING",
      "value": "case"
    },
    "keyword_default": {
      "type": "STRING",
      "value": "default"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Za-z]+[_A-Za-z]*"
    },
    "number": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "0"
        },
        {
          "type": "PATTERN",
          "value": "[1-9]+[0-9]*"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "block_comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}